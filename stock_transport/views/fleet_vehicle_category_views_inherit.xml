<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="your_module_fleet_vehicle_model_category_view_tree" model="ir.ui.view">
        <field name="name">fleet.vehicle.model.category.view.tree.inherited</field>
        <field name="model">fleet.vehicle.model.category</field>
        <field name="inherit_id" ref="fleet.fleet_vehicle_model_category_view_tree" />
        <field name="arch" type="xml">
            <xpath expr="//tree/field[@name='name']" position="after">
                <field name='max_weight' />
                <field name='max_volume' />
            </xpath>
        </field>
    </record>
</odoo>